{
    "name": "changelog/formatter",
    "description": "TBD",
    "keywords": [
        "changelog",
        "formatter",
        "git"
    ],
    "homepage": "https://github.com/changelog-page/formatter",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Changelog\\Formatter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Changelog\\Formatter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix ./"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
